<!-- Character entity set. Typical invocation:
     <!ENTITY % ISOGRK5 PUBLIC
       "ISO 9573-15:1993//ENTITIES Extra Classical Greek Letters //EN">
     %ISOGRK5;
-->
<!Entity cogr ":" -- Gr colon or raised dot -->
<!Entity qmgr ";" -- Gr question mark -->
<!Entity bmgr "b" -- Gr sm beta at middle of word -->
<!Entity diggr "#" -- Gr sm digamma -->
<!Entity DIGgr "#" -- Gr cap digamma -->
<!Entity kogr "&" -- Gr sm koppa -->
<!Entity KOgr "&" -- Gr cap koppa -->
<!Entity kovgr "&" -- Gr sm koppa (variant) -->
<!Entity KOVgr "&" -- Gr cap koppa (variant) -->
<!Entity stgr "$" -- Gr sm stigma -->
<!Entity STgr "$" -- Gr cap stigma -->
<!Entity samgr '"' -- Gr sm sampi -->
<!Entity SAMgr '"' -- Gr cap sampi -->
<!Entity aiotgr "v" -- Gr sm letter alpha with iota -->
<!Entity Aiotgr "Ai" -- Gr cap alpha with iota -->
<!Entity arigr "¬" -- Gr sm alpha with rough (asper) and iota -->
<!Entity Arigr "`Ai" -- Gr cap alpha with rough (asper) and iota -->
<!Entity asigr "­" -- Gr sm alpha with smooth (lenis) and iota -->
<!Entity Asigr "'Ai" -- Gr cap alpha with smooth (lenis) and iota -->
<!Entity aaigr "®" -- Gr sm alpha with acute and iota -->
<!Entity Aaigr "/Ai" -- Gr cap alpha with acute and iota -->
<!Entity agigr "±" -- Gr sm alpha with grave and iota -->
<!Entity Agigr "\Ai" -- Gr cap alpha with grave and iota -->
<!Entity acigr "´" -- Gr sm alpha with circum and iota -->
<!Entity Acigr "^Ai" -- Gr cap alpha with circum and iota -->
<!Entity araigr "¯" -- Gr sm alpha with rough (asper), acute and iota -->
<!Entity Araigr "`/Ai" -- Gr cap alpha with rough (asper), acute and iota -->
<!Entity asigr "°" -- Gr sm alpha with smooth (lenis), acute and iota -->
<!Entity Asigr "'/Ai" -- Gr cap alpha with smooth (lenis), acute and iota -->
<!Entity argigr "²" -- Gr sm alpha with rough (asper), grave and iota -->
<!Entity Argigr "`\Ai" -- Gr cap alpha with rough (asper), grave and iota -->
<!Entity asgigr "³" -- Gr sm alpha with smooth (lenis), grave and iota -->
<!Entity Asgigr "'\Ai" -- Gr cap alpha with smooth (lenis), grave and iota -->
<!Entity arcigr "µ" -- Gr sm alpha with rough (asper), circum and iota -->
<!Entity Arcigr "`^Ai" -- Gr cap alpha with rough (asper), circum and iota -->
<!Entity ascigr "¶" -- Gr sm alpha with smooth (lenis), circum and iota -->
<!Entity Ascigr "'^Ai" -- Gr cap alpha with smooth (lenis), circum and iota -->

<!Entity arougr "¡" -- Gr sm alpha with rough (asper) -->
<!Entity Arougr "`A" -- Gr cap alpha with rough (asper) -->
<!Entity asmogr "¢" -- Gr sm alpha with smooth (lenis) -->
<!Entity Asmogr "'A" -- Gr cap alpha with smooth (lenis) -->
<!Entity agragr "¦" -- Gr sm alpha with grave -->
<!Entity Agragr "\A" -- Gr cap alpha with grave -->
<!Entity aacugr "£" -- Gr sm alpha with acute -->
<!Entity Aacugr "/A" -- Gr cap alpha with acute -->
<!Entity acirgr "©" -- Gr sm alpha with circum -->
<!Entity Acirgr "^A" -- Gr cap alpha with circum -->
<!Entity aragr "¤" -- Gr sm alpha with rough (asper) and acute -->
<!Entity Aragr "`/A" -- Gr cap alpha with rough (asper) and acute -->
<!Entity asagr "¥" -- Gr sm alpha with smooth (lenis) and acute -->
<!Entity Asagr "'/A" -- Gr cap alpha with smooth (lenis) and acute -->
<!Entity arggr "§" -- Gr sm alpha with rough (asper) and grave -->
<!Entity Arggr "`\A" -- Gr cap alpha with rough (asper) and grave -->
<!Entity asggr "¨" -- Gr sm alpha with smooth (lenis) and grave -->
<!Entity Asggr "'\A" -- Gr cap alpha with smooth (lenis) and grave -->
<!Entity arcgr "ª" -- Gr sm alpha with rough (asper) and circum -->
<!Entity Arcgr "`^A" -- Gr cap alpha with rough (asper) and circum -->
<!Entity ascgr "«" -- Gr sm alpha with smooth (lenis) and circum -->
<!Entity Ascgr "'^A" -- Gr cap alpha with smooth (lenis) and circum -->

<!Entity erougr "˜" -- Gr sm epsilon with rough (asper) -->
<!Entity Erougr "`E" -- Gr cap epsilon with rough (asper) -->
<!Entity esmogr "™" -- Gr sm epsilon with smooth (lenis) -->
<!Entity Esmogr "'E" -- Gr cap epsilon with smooth (lenis) -->
<!Entity egragr "" -- Gr sm epsilon with grave -->
<!Entity Egragr "\E" -- Gr cap epsilon with grave -->
<!Entity eacugr "š" -- Gr sm epsilon with acute -->
<!Entity Eacugr "/E" -- Gr cap epsilon with acute -->
<!Entity ecirgr "^e" -- Gr sm epsilon with circum -->
<!Entity Ecirgr "^E" -- Gr cap epsilon with circum -->
<!Entity eragr "›" -- Gr sm epsilon with rough (asper) and acute -->
<!Entity Eragr "`/E" -- Gr cap epsilon with rough (asper) and acute -->
<!Entity esagr "œ" -- Gr sm epsilon with smooth (lenis) and acute -->
<!Entity Esagr "'/E" -- Gr cap epsilon with smooth (lenis) and acute -->
<!Entity erggr "ž" -- Gr sm epsilon with rough (asper) and grave -->
<!Entity Erggr "`\E" -- Gr cap epsilon with rough (asper) and grave -->
<!Entity esggr "Ÿ" -- Gr sm epsilon with smooth (lenis) and grave -->
<!Entity Esggr "'\E" -- Gr cap epsilon with smooth (lenis) and grave -->

<!Entity eeiotgr "V" -- Gr sm letter eta with iota -->
<!Entity EEiotgr "Hi" -- Gr cap eta with iota -->
<!Entity eerigr "Å" -- Gr sm eta with rough (asper) and iota -->
<!Entity EErigr "`Hi" -- Gr cap eta with rough (asper) and iota -->
<!Entity eesigr "Æ" -- Gr sm eta with smooth (lenis) and iota -->
<!Entity EEsigr "'Hi" -- Gr cap eta with smooth (lenis) and iota -->
<!Entity eeaigr "Ç" -- Gr sm eta with acute and iota -->
<!Entity EEaigr "/Hi" -- Gr cap eta with acute and iota -->
<!Entity eegigr "Ê" -- Gr sm eta with grave and iota -->
<!Entity EEgigr "\Hi" -- Gr cap eta with grave and iota -->
<!Entity eecigr "Í" -- Gr sm eta with circum and iota -->
<!Entity EEcigr "^Hi" -- Gr cap eta with circum and iota -->
<!Entity eeraigr "È" -- Gr sm eta with rough (asper), acute and iota -->
<!Entity EEraigr "`/Hi" -- Gr cap eta with rough (asper), acute and iota -->
<!Entity eesigr "É" -- Gr sm eta with smooth (lenis), acute and iota -->
<!Entity EEsigr "'/Hi" -- Gr cap eta with smooth (lenis), acute and iota -->
<!Entity eergigr "Ë" -- Gr sm eta with rough (asper), grave and iota -->
<!Entity EErgigr "`\Hi" -- Gr cap eta with rough (asper), grave and iota -->
<!Entity eesgigr "Ì" -- Gr sm eta with smooth (lenis), grave and iota -->
<!Entity EEsgigr "'\Hi" -- Gr cap eta with smooth (lenis), grave and iota -->
<!Entity eercigr "Î" -- Gr sm eta with rough (asper), circum and iota -->
<!Entity EErcigr "`^Hi" -- Gr cap eta with rough (asper), circum and iota -->
<!Entity eescigr "Ï" -- Gr sm eta with smooth (lenis), circum and iota -->
<!Entity EEscigr "'^Hi" -- Gr cap eta with smooth (lenis), circum and iota -->
<!Entity eerougr "¹" -- Gr sm eta with rough (asper) -->
<!Entity EErougr "`H" -- Gr cap eta with rough (asper) -->
<!Entity eesmogr "º" -- Gr sm eta with smooth (lenis) -->
<!Entity EEsmogr "'H" -- Gr cap eta with smooth (lenis) -->
<!Entity eegragr "¾" -- Gr sm eta with grave -->
<!Entity EEgragr "\H" -- Gr cap eta with grave -->
<!Entity eeacugr "»" -- Gr sm eta with acute -->
<!Entity EEacugr "/H" -- Gr cap eta with acute -->
<!Entity eecirgr "Á" -- Gr sm eta with circum -->
<!Entity EEcirgr "^H" -- Gr cap eta with circum -->
<!Entity eeragr "¼" -- Gr sm eta with rough (asper) and acute -->
<!Entity EEragr "`/H" -- Gr cap eta with rough (asper) and acute -->
<!Entity eesagr "½" -- Gr sm eta with smooth (lenis) and acute -->
<!Entity EEsagr "'/H" -- Gr cap eta with smooth (lenis) and acute -->
<!Entity eerggr "¿" -- Gr sm eta with rough (asper) and grave -->
<!Entity EErggr "`\H" -- Gr cap eta with rough (asper) and grave -->
<!Entity eesggr "À" -- Gr sm eta with smooth (lenis) and grave -->
<!Entity EEsggr "'\H" -- Gr cap eta with smooth (lenis) and grave -->
<!Entity eercgr "Â" -- Gr sm eta with rough (asper) and circum -->
<!Entity EErcgr "`^H" -- Gr cap eta with rough (asper) and circum -->
<!Entity eescgr "Ã" -- Gr sm eta with smooth (lenis) and circum -->
<!Entity EEscgr "'^H" -- Gr cap eta with smooth (lenis) and circum -->

<!Entity irougr "ƒ" -- Gr sm iota with rough (asper) -->
<!Entity Irougr "`I" -- Gr cap iota with rough (asper) -->
<!Entity ismogr "„" -- Gr sm iota with smooth (lenis) -->
<!Entity Ismogr "'I" -- Gr cap iota with smooth (lenis) -->
<!Entity igragr "ˆ" -- Gr sm iota with grave -->
<!Entity Igragr "\I" -- Gr cap iota with grave -->
<!Entity iacugr "…" -- Gr sm iota with acute -->
<!Entity Iacugr "/I" -- Gr cap iota with acute -->
<!Entity icirgr "‹" -- Gr sm iota with circum -->
<!Entity Icirgr "^I" -- Gr cap iota with circum -->
<!Entity iragr "†" -- Gr sm iota with rough (asper) and acute -->
<!Entity Iragr "`/I" -- Gr cap iota with rough (asper) and acute -->
<!Entity isagr "‡" -- Gr sm iota with smooth (lenis) and acute -->
<!Entity Isagr "'/I" -- Gr cap iota with smooth (lenis) and acute -->
<!Entity irggr "‰" -- Gr sm iota with rough (asper) and grave -->
<!Entity Irggr "`\I" -- Gr cap iota with rough (asper) and grave -->
<!Entity isggr "Š" -- Gr sm iota with smooth (lenis) and grave -->
<!Entity Isggr "'\I" -- Gr cap iota with smooth (lenis) and grave -->
<!Entity ircgr "Œ" -- Gr sm iota with rough (asper) and circum -->
<!Entity Ircgr "`^I" -- Gr cap iota with rough (asper) and circum -->
<!Entity iscgr "" -- Gr sm iota with smooth (lenis) and circum -->
<!Entity Iscgr "'^I" -- Gr cap iota with smooth (lenis) and circum -->

<!Entity igdgr "" -- Gr sm iota with grave and diaresis -->
<!Entity Igdgr "@\I" -- Gr cap iota with grave and diaresis -->
<!Entity iadgr "" -- Gr sm iota with acute and diaresis -->
<!Entity Iadgr "—I" -- Gr cap iota with acute and diaresis -->
<!Entity icdgr "^Ž" -- Gr sm iota with circum and diaresis -->
<!Entity Icdgr "@^I" -- Gr cap iota with circum and diaresis -->

<!Entity orougr "Ð" -- Gr sm omicron with rough (asper) -->
<!Entity Orougr "`O" -- Gr cap omicron with rough (asper) -->
<!Entity osmogr "Ñ" -- Gr sm omicron with smooth (lenis) -->
<!Entity Osmogr "'O" -- Gr cap omicron with smooth (lenis) -->
<!Entity ogragr "Õ" -- Gr sm omicron with grave -->
<!Entity Ogragr "\O" -- Gr cap omicron with grave -->
<!Entity oacugr "Ò" -- Gr sm omicron with acute -->
<!Entity Oacugr "/O" -- Gr cap omicron with acute -->
<!Entity ocirgr "^o" -- Gr sm omicron with circum -->
<!Entity Ocirgr "^O" -- Gr cap omicron with circum -->
<!Entity oragr "Ó" -- Gr sm omicron with rough (asper) and acute -->
<!Entity Oragr "`/O" -- Gr cap omicron with rough (asper) and acute -->
<!Entity osagr "Ô" -- Gr sm omicron with smooth (lenis) and acute -->
<!Entity Osagr "'/O" -- Gr cap omicron with smooth (lenis) and acute -->
<!Entity orggr "Ö" -- Gr sm omicron with rough (asper) and grave -->
<!Entity Orggr "`\O" -- Gr cap omicron with rough (asper) and grave -->
<!Entity osggr "×" -- Gr sm omicron with smooth (lenis) and grave -->
<!Entity Osggr "'\O" -- Gr cap omicron with smooth (lenis) and grave -->

<!Entity urougr "Ø" -- Gr sm upsilon with rough (asper) -->
<!Entity Urougr "`U" -- Gr cap upsilon with rough (asper) -->
<!Entity usmogr "Ù" -- Gr sm upsilon with smooth (lenis) -->
<!Entity Usmogr "'U" -- Gr cap upsilon with smooth (lenis) -->
<!Entity ugragr "Ý" -- Gr sm upsilon with grave -->
<!Entity Ugragr "\U" -- Gr cap upsilon with grave -->
<!Entity uacugr "Ú" -- Gr sm upsilon with acute -->
<!Entity Uacugr "/U" -- Gr cap upsilon with acute -->
<!Entity ucirgr "à" -- Gr sm upsilon with circum -->
<!Entity Ucirgr "^U" -- Gr cap upsilon with circum -->
<!Entity uragr "Û" -- Gr sm upsilon with rough (asper) and acute -->
<!Entity Uragr "`/U" -- Gr cap upsilon with rough (asper) and acute -->
<!Entity usagr "Ü" -- Gr sm upsilon with smooth (lenis) and acute -->
<!Entity Usagr "'/U" -- Gr cap upsilon with smooth (lenis) and acute -->
<!Entity urggr "Þ" -- Gr sm upsilon with rough (asper) and grave -->
<!Entity Urggr "`\U" -- Gr cap upsilon with rough (asper) and grave -->
<!Entity usggr "ß" -- Gr sm upsilon with smooth (lenis) and grave -->
<!Entity Usggr "'\U" -- Gr cap upsilon with smooth (lenis) and grave -->
<!Entity urcgr "á" -- Gr sm upsilon with rough (asper) and circum -->
<!Entity Urcgr "`^U" -- Gr cap upsilon with rough (asper) and circum -->
<!Entity uscgr "â" -- Gr sm upsilon with smooth (lenis) and circum -->
<!Entity Uscgr "'^U" -- Gr cap upsilon with smooth (lenis) and circum -->

<!Entity ugdgr "å" -- Gr sm upsilon with grave and diaresis -->
<!Entity Ugdgr "\U" -- Gr cap upsilon with grave and diaresis -->
<!Entity ucdgr "^ã" -- Gr sm upsilon with circum and diaresis -->
<!Entity Ucdgr "^U" -- Gr cap upsilon with circum and diaresis -->
<!Entity uadgr "ä" -- Gr sm upsilon with acute and diaresis -->
<!Entity Uadgr "/U" -- Gr cap upsilon with acute and diaresis -->

